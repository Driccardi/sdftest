<?xml version="1.0" encoding="UTF-8"?>
<customrecordtype scriptid="customrecord_sourcing_tracker">
  <accesstype>CUSTRECORDENTRYPERM</accesstype>
  <allowattachments>T</allowattachments>
  <allowinlinedeleting>F</allowinlinedeleting>
  <allowinlinedetaching>T</allowinlinedetaching>
  <allowinlineediting>T</allowinlineediting>
  <allowmobileaccess>F</allowmobileaccess>
  <allownumberingoverride>F</allownumberingoverride>
  <allowquickadd>T</allowquickadd>
  <allowquicksearch>T</allowquicksearch>
  <allowuiaccess>T</allowuiaccess>
  <description>Tracks vendor qualification and product sourcing attempts. Each record represents a unique Vendor + Item + Model combination for a specific sourcing round.</description>
  <enabledle>T</enabledle>
  <enablekeywords>T</enablekeywords>
  <enablemailmerge>F</enablemailmerge>
  <enablenametranslation>F</enablenametranslation>
  <enablenumbering>T</enablenumbering>
  <enableoptimisticlocking>T</enableoptimisticlocking>
  <enablesystemnotes>T</enablesystemnotes>
  <hierarchical>F</hierarchical>
  <iconbuiltin>T</iconbuiltin>
  <includeinsearchmenu>T</includeinsearchmenu>
  <includename>T</includename>
  <isinactive>F</isinactive>
  <isordered>F</isordered>
  <numberingprefix>ST-</numberingprefix>
  <numberingmindigits>4</numberingmindigits>
  <recordname>Sourcing Tracker</recordname>
  <showcreationdate>F</showcreationdate>
  <showcreationdateonlist>F</showcreationdateonlist>
  <showid>T</showid>
  <showlastmodified>F</showlastmodified>
  <showlastmodifiedonlist>F</showlastmodifiedonlist>
  <shownotes>T</shownotes>
  <showowner>F</showowner>
  <showownerallowchange>F</showownerallowchange>
  <showowneronlist>F</showowneronlist>
  <customrecordcustomfields>
    <customrecordcustomfield scriptid="custrecord_st_vendor">
      <accesslevel>2</accesslevel>
      <allowquickadd>F</allowquickadd>
      <applyformatting>F</applyformatting>
      <checkspelling>F</checkspelling>
      <defaultchecked>F</defaultchecked>
      <description>The vendor being evaluated for this item</description>
      <displaytype>NORMAL</displaytype>
      <fieldtype>SELECT</fieldtype>
      <globalsearch>F</globalsearch>
      <help>Select the vendor you are sourcing this item from</help>
      <isformula>F</isformula>
      <ismandatory>T</ismandatory>
      <isparent>F</isparent>
      <label>Vendor</label>
      <onparentdelete>SET_NULL</onparentdelete>
      <rolerestrict>F</rolerestrict>
      <searchlevel>2</searchlevel>
      <selectrecordtype>-23</selectrecordtype>
      <showinlist>T</showinlist>
      <storevalue>T</storevalue>
    </customrecordcustomfield>
    <customrecordcustomfield scriptid="custrecord_st_item">
      <accesslevel>2</accesslevel>
      <allowquickadd>F</allowquickadd>
      <applyformatting>F</applyformatting>
      <checkspelling>F</checkspelling>
      <defaultchecked>F</defaultchecked>
      <description>The inventory item being sourced</description>
      <displaytype>NORMAL</displaytype>
      <fieldtype>SELECT</fieldtype>
      <globalsearch>F</globalsearch>
      <help>Select the NetSuite item record for this product</help>
      <isformula>F</isformula>
      <ismandatory>T</ismandatory>
      <isparent>F</isparent>
      <label>Item</label>
      <onparentdelete>SET_NULL</onparentdelete>
      <rolerestrict>F</rolerestrict>
      <searchlevel>2</searchlevel>
      <selectrecordtype>-10</selectrecordtype>
      <showinlist>T</showinlist>
      <storevalue>T</storevalue>
    </customrecordcustomfield>
    <customrecordcustomfield scriptid="custrecord_st_model_code">
      <accesslevel>2</accesslevel>
      <allowquickadd>F</allowquickadd>
      <applyformatting>F</applyformatting>
      <checkspelling>F</checkspelling>
      <defaultchecked>F</defaultchecked>
      <description>Vendor's model or style identifier</description>
      <displaytype>NORMAL</displaytype>
      <fieldtype>TEXT</fieldtype>
      <globalsearch>F</globalsearch>
      <help>Enter the vendor's model or style number (may differ from NetSuite SKU)</help>
      <isformula>F</isformula>
      <ismandatory>F</ismandatory>
      <isparent>F</isparent>
      <label>Model/Style Code</label>
      <maxlength>50</maxlength>
      <rolerestrict>F</rolerestrict>
      <searchlevel>2</searchlevel>
      <showinlist>T</showinlist>
      <storevalue>T</storevalue>
    </customrecordcustomfield>
    <customrecordcustomfield scriptid="custrecord_st_status">
      <accesslevel>2</accesslevel>
      <allowquickadd>F</allowquickadd>
      <applyformatting>F</applyformatting>
      <checkspelling>F</checkspelling>
      <defaultchecked>F</defaultchecked>
      <defaultselection>[scriptid=customlist_ns_csi_sourcing_status].[scriptid=_contacted]</defaultselection>
      <description>Current stage in the sourcing workflow</description>
      <displaytype>NORMAL</displaytype>
      <fieldtype>SELECT</fieldtype>
      <globalsearch>F</globalsearch>
      <help>Select the current status of this sourcing effort</help>
      <isformula>F</isformula>
      <ismandatory>T</ismandatory>
      <isparent>F</isparent>
      <label>Sourcing Status</label>
      <onparentdelete>NO_ACTION</onparentdelete>
      <rolerestrict>F</rolerestrict>
      <searchlevel>2</searchlevel>
      <selectrecordtype>[scriptid=customlist_ns_csi_sourcing_status]</selectrecordtype>
      <showinlist>T</showinlist>
      <storevalue>T</storevalue>
    </customrecordcustomfield>
    <customrecordcustomfield scriptid="custrecord_st_buyer">
      <accesslevel>2</accesslevel>
      <allowquickadd>F</allowquickadd>
      <applyformatting>F</applyformatting>
      <checkspelling>F</checkspelling>
      <defaultchecked>F</defaultchecked>
      <description>Employee responsible for this sourcing effort</description>
      <displaytype>NORMAL</displaytype>
      <dynamicdefault>STDUSERUSER</dynamicdefault>
      <fieldtype>SELECT</fieldtype>
      <globalsearch>F</globalsearch>
      <help>Select the buyer or purchasing agent managing this sourcing request</help>
      <isformula>F</isformula>
      <ismandatory>T</ismandatory>
      <isparent>F</isparent>
      <label>Assigned Buyer</label>
      <onparentdelete>SET_NULL</onparentdelete>
      <rolerestrict>F</rolerestrict>
      <searchlevel>2</searchlevel>
      <selectrecordtype>-4</selectrecordtype>
      <showinlist>T</showinlist>
      <storevalue>T</storevalue>
    </customrecordcustomfield>
    <customrecordcustomfield scriptid="custrecord_st_contact_date">
      <accesslevel>2</accesslevel>
      <allowquickadd>F</allowquickadd>
      <applyformatting>F</applyformatting>
      <checkspelling>F</checkspelling>
      <defaultchecked>F</defaultchecked>
      <description>Date when vendor was first contacted</description>
      <displaytype>NORMAL</displaytype>
      <dynamicdefault>STDCURRENTDATE</dynamicdefault>
      <fieldtype>DATE</fieldtype>
      <globalsearch>F</globalsearch>
      <help>Date you first reached out to the vendor for this item</help>
      <isformula>F</isformula>
      <ismandatory>T</ismandatory>
      <isparent>F</isparent>
      <label>Initial Contact Date</label>
      <rolerestrict>F</rolerestrict>
      <searchlevel>2</searchlevel>
      <showinlist>T</showinlist>
      <storevalue>T</storevalue>
    </customrecordcustomfield>
    <customrecordcustomfield scriptid="custrecord_st_quote_date">
      <accesslevel>2</accesslevel>
      <allowquickadd>F</allowquickadd>
      <applyformatting>F</applyformatting>
      <checkspelling>F</checkspelling>
      <defaultchecked>F</defaultchecked>
      <description>Date vendor responded with pricing</description>
      <displaytype>NORMAL</displaytype>
      <fieldtype>DATE</fieldtype>
      <globalsearch>F</globalsearch>
      <help>Date the vendor provided a formal quote</help>
      <isformula>F</isformula>
      <ismandatory>F</ismandatory>
      <isparent>F</isparent>
      <label>Quote Received Date</label>
      <rolerestrict>F</rolerestrict>
      <searchlevel>2</searchlevel>
      <showinlist>T</showinlist>
      <storevalue>T</storevalue>
    </customrecordcustomfield>
    <customrecordcustomfield scriptid="custrecord_st_quoted_cost">
      <accesslevel>2</accesslevel>
      <allowquickadd>F</allowquickadd>
      <applyformatting>T</applyformatting>
      <checkspelling>F</checkspelling>
      <defaultchecked>F</defaultchecked>
      <description>Vendor's quoted price per unit</description>
      <displaytype>NORMAL</displaytype>
      <fieldtype>CURRENCY</fieldtype>
      <globalsearch>F</globalsearch>
      <help>Enter the vendor's quoted unit price</help>
      <isformula>F</isformula>
      <ismandatory>F</ismandatory>
      <isparent>F</isparent>
      <label>Quoted Unit Cost</label>
      <rolerestrict>F</rolerestrict>
      <searchlevel>2</searchlevel>
      <showinlist>T</showinlist>
      <storevalue>T</storevalue>
    </customrecordcustomfield>
    <customrecordcustomfield scriptid="custrecord_st_target_cost">
      <accesslevel>2</accesslevel>
      <allowquickadd>F</allowquickadd>
      <applyformatting>T</applyformatting>
      <checkspelling>F</checkspelling>
      <defaultchecked>F</defaultchecked>
      <description>Internal target or budget price</description>
      <displaytype>NORMAL</displaytype>
      <fieldtype>CURRENCY</fieldtype>
      <globalsearch>F</globalsearch>
      <help>Enter your target or maximum cost for this item</help>
      <isformula>F</isformula>
      <ismandatory>F</ismandatory>
      <isparent>F</isparent>
      <label>Target Cost</label>
      <rolerestrict>F</rolerestrict>
      <searchlevel>2</searchlevel>
      <showinlist>T</showinlist>
      <storevalue>T</storevalue>
    </customrecordcustomfield>
    <customrecordcustomfield scriptid="custrecord_st_moq">
      <accesslevel>2</accesslevel>
      <allowquickadd>F</allowquickadd>
      <applyformatting>T</applyformatting>
      <checkspelling>F</checkspelling>
      <defaultchecked>F</defaultchecked>
      <description>Minimum order quantity required by vendor</description>
      <displaytype>NORMAL</displaytype>
      <fieldtype>INTEGER</fieldtype>
      <globalsearch>F</globalsearch>
      <help>Enter the vendor's minimum order quantity</help>
      <isformula>F</isformula>
      <ismandatory>F</ismandatory>
      <isparent>F</isparent>
      <label>MOQ</label>
      <rolerestrict>F</rolerestrict>
      <searchlevel>2</searchlevel>
      <showinlist>F</showinlist>
      <storevalue>T</storevalue>
    </customrecordcustomfield>
    <customrecordcustomfield scriptid="custrecord_st_lead_time">
      <accesslevel>2</accesslevel>
      <allowquickadd>F</allowquickadd>
      <applyformatting>T</applyformatting>
      <checkspelling>F</checkspelling>
      <defaultchecked>F</defaultchecked>
      <description>Quoted lead time in calendar days</description>
      <displaytype>NORMAL</displaytype>
      <fieldtype>INTEGER</fieldtype>
      <globalsearch>F</globalsearch>
      <help>Number of days from PO to delivery (per vendor quote)</help>
      <isformula>F</isformula>
      <ismandatory>F</ismandatory>
      <isparent>F</isparent>
      <label>Lead Time (Days)</label>
      <rolerestrict>F</rolerestrict>
      <searchlevel>2</searchlevel>
      <showinlist>F</showinlist>
      <storevalue>T</storevalue>
    </customrecordcustomfield>
