<?xml version="1.0"?>
<!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd">
<pdf>
  <head>
    <style>table { font-size: 9pt; table-layout: fixed; width: 100%; }
th { font-weight: bold; font-size: 8pt; vertical-align: middle; padding: 5px 6px 3px; background-color: #e3e3e3; color: #333333; padding-bottom: 10px; padding-top: 10px; }
td { padding: 4px 6px; }
b { font-weight: bold; color: #333333; }
</style>
  </head>
  <body width="612" height="792">
    <table style="font-size: 10pt; width: 100%;">
      <tr>
        <td style="align:center;"> <span style="font-size: 28pt;">${record@title} </span></td>
      </tr>
    </table>
    <table style="font-size: 12pt; width: 100%;">
      <tr>
        <td style="align:center;">This Agreement is made and entered on  ${record.created?string("MM/dd/yyyy")} by and between:</td>
      </tr>
    </table>

    <!-- Compact, side-by-side details (Advanced PDF safe) -->
    <table width="100%" cellpadding="0" cellspacing="0"
           style="font-size:10pt; line-height:13pt; table-layout:fixed; border:0; page-break-inside:avoid; -fs-page-break-inside:avoid;">
      <!-- Column plan: [Label 22%][Value 28%][Label 22%][Value 28%] -->
      <tr>
        <td colspan="4"
            style="font-weight:bold; background:#f5f6f7; padding:6pt 6pt; border-top:0.75pt solid #ddd; border-bottom:0.75pt solid #ddd; page-break-inside:avoid;">
          Owner
        </td>
      </tr>
      <tr style="page-break-inside:avoid;">
        <td style="width:22%; font-weight:bold; padding:4pt 6pt; vertical-align:top; text-align:left;">Company Name:</td>
        <td style="width:28%; padding:4pt 6pt; vertical-align:top; text-align:left; word-wrap:break-word; white-space:normal;">
          ${companyInformation.companyname}
        </td>
        <td style="width:22%; font-weight:bold; padding:4pt 6pt; vertical-align:top; text-align:left;">Address:</td>
        <td style="width:28%; padding:4pt 6pt; vertical-align:top; text-align:left; word-wrap:break-word; white-space:normal;">
          ${companyInformation.mainaddress_text}
        </td>
      </tr>

      <tr>
        <td colspan="4"
            style="font-weight:bold; background:#f5f6f7; padding:6pt 6pt; border-top:0.75pt solid #ddd; border-bottom:0.75pt solid #ddd; page-break-inside:avoid;">
          Renter
        </td>
      </tr>
      <tr style="page-break-inside:avoid;">
        <td style="font-weight:bold; padding:4pt 6pt; vertical-align:top; text-align:left;">Name:</td>
        <td style="padding:4pt 6pt; vertical-align:top; text-align:left; word-wrap:break-word; white-space:normal;">
          ${record.custrecord_ar_customer}
        </td>
        <td style="font-weight:bold; padding:4pt 6pt; vertical-align:top; text-align:left;">Address:</td>
        <td style="padding:4pt 6pt; vertical-align:top; text-align:left; word-wrap:break-word; white-space:normal;">
          ${record.custrecord_ar_address}
        </td>
      </tr>

      <tr>
        <td colspan="4"
            style="font-weight:bold; background:#f5f6f7; padding:6pt 6pt; border-top:0.75pt solid #ddd; border-bottom:0.75pt solid #ddd; page-break-inside:avoid;">
          Equipment Details
        </td>
      </tr>
      <tr style="page-break-inside:avoid;">
        <td style="font-weight:bold; padding:4pt 6pt; vertical-align:top; text-align:left;">Item:</td>
        <td style="padding:4pt 6pt; vertical-align:top; text-align:left;">
          ${record.custrecord_serialized_item}
        </td>
        <td style="font-weight:bold; padding:4pt 6pt; vertical-align:top; text-align:left;">Serial Number:</td>
        <td style="padding:4pt 6pt; vertical-align:top; text-align:left;">
          ${record.custrecord_ar_serial_number}
        </td>
      </tr>

      <tr>
        <td colspan="4"
            style="font-weight:bold; background:#f5f6f7; padding:6pt 6pt; border-top:0.75pt solid #ddd; border-bottom:0.75pt solid #ddd; page-break-inside:avoid;">
          Rental Term
        </td>
      </tr>
      <tr style="page-break-inside:avoid;">
        <td style="font-weight:bold; padding:4pt 6pt; vertical-align:top; text-align:left;">Start Date:</td>
        <td style="padding:4pt 6pt; vertical-align:top; text-align:left;">
          ${record.custrecord_rental_start_date}
        </td>
        <td style="font-weight:bold; padding:4pt 6pt; vertical-align:top; text-align:left;">End Date:</td>
        <td style="padding:4pt 6pt; vertical-align:top; text-align:left;">
          ${record.custrecord_rental_end_date}
        </td>
      </tr>

      <tr>
        <td colspan="4"
            style="font-weight:bold; background:#f5f6f7; padding:6pt 6pt; border-top:0.75pt solid #ddd; border-bottom:0.75pt solid #ddd; page-break-inside:avoid;">
          Rental Fee
        </td>
      </tr>
      <tr style="page-break-inside:avoid;">
        <td style="font-weight:bold; padding:4pt 6pt; vertical-align:top; text-align:left;">Fee:</td>
        <td style="padding:4pt 6pt; vertical-align:top; text-align:left;">
          ${record.custrecord_ar_fee} per ${record.custrecord_ar_rental_rate_per}
        </td>
        <td style="font-weight:bold; padding:4pt 6pt; vertical-align:top; text-align:left;">Billing Frequency:</td>
        <td style="padding:4pt 6pt; vertical-align:top; text-align:left;">
          ${record.custrecord_ar_billing_frequency}
        </td>
      </tr>
    </table>



    <!-- Responsibilities Section -->
    <table width="100%" cellpadding="0" cellspacing="0"
           style="font-size:10pt; line-height:14pt; border:0; margin-top:8pt; table-layout:fixed; page-break-inside:avoid;">
      <tr>
        <td style="font-weight:bold; background:#f5f6f7; padding:6pt 6pt; border-top:0.75pt solid #ddd; border-bottom:0.75pt solid #ddd;">
          Responsibilities
        </td>
      </tr>
      <tr>
        <td style="padding:8pt 6pt; font-size:9pt; line-height:13pt; text-align:justify; white-space:pre-wrap; word-wrap:break-word; border:0.5pt solid #ddd; border-top:none;">
          ${record.custrecord_rental_terms_conditions}
        </td>
      </tr>
    </table>

    <!-- Termination Section -->
    <table width="100%" cellpadding="0" cellspacing="0"
           style="font-size:10pt; line-height:14pt; border:0; margin-top:8pt; table-layout:fixed; page-break-inside:avoid;">
      <tr>
        <td style="width:22%; font-weight:bold; background:#f5f6f7; padding:6pt; border-top:0.75pt solid #ddd; border-bottom:0.75pt solid #ddd;">
          Termination
        </td>
        <td style="width:78%; padding:6pt; text-align:justify; border-top:0.75pt solid #ddd; border-bottom:0.75pt solid #ddd; border-left:0.5pt solid #ddd;">
          Owner may terminate this agreement if terms are violated. Equipment must be returned immediately upon request.
        </td>
      </tr>
    </table>


    <!-- Signatures Section -->
    <table width="100%" cellpadding="0" cellspacing="0"
           style="font-family:Arial, sans-serif; font-size:10pt; margin-top:12pt; line-height:14pt; table-layout:fixed; page-break-inside:avoid;">
      <tr>
        <td colspan="4"
            style="font-weight:bold; color:#333; background:#f5f6f7; padding:6pt; border-top:0.75pt solid #ddd; border-bottom:0.75pt solid #ddd;">
          Signatures
        </td>
      </tr>

      <!-- Owner Signature -->
      <tr>
        <td style="padding-top:12pt; width:20%; font-weight:bold;">Owner Signature:</td>
        <td style="width:30%; border-bottom:1pt solid #000;">&nbsp;</td>
        <td style="width:10%; font-weight:bold; text-align:right;">Date:</td>
        <td style="width:40%; border-bottom:1pt solid #000;">&nbsp;</td>
      </tr>
      <tr>
        <td style="padding-top:6pt; font-weight:bold;">Print Name:</td>
        <td style="border-bottom:1pt solid #000;">&nbsp;</td>
        <td colspan="2"></td>
      </tr>

      <!-- Renter Signature -->
      <tr>
        <td style="padding-top:18pt; font-weight:bold;">Renter Signature:</td>
        <td style="border-bottom:1pt solid #000;">&nbsp;</td>
        <td style="font-weight:bold; text-align:right;">Date:</td>
        <td style="border-bottom:1pt solid #000;">&nbsp;</td>
      </tr>
      <tr>
        <td style="padding-top:6pt; font-weight:bold;">Print Name:</td>
        <td style="border-bottom:1pt solid #000;">&nbsp;</td>
        <td colspan="2"></td>
      </tr>
    </table>


  </body>
</pdf>
