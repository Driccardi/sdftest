<publisheddashboard scriptid="custpubdashboard_ns_csgn_cosndashboard">
  <center>BASIC</center>
  <locknewbar>F</locknewbar>
  <lockshortcuts>F</lockshortcuts>
  <name>Consignment Dashboard</name>
  <notes>This publishes the Consignment dashboard which includes a suite of notifications, reminders, sales history, and location inventory details.</notes>
  <roles>
    <role>
      <role>ADMINISTRATOR</role>
    </role>
  </roles>
  <dashboards>
    <dashboard>
      <centertab>[scriptid=custcentertab_ns_csgn_consignment]</centertab>
      <layout>TWO_COLUMN</layout>
      <mode>UNLOCKED</mode>
      <leftcolumn>
        <reminders>
          <isminimized>F</isminimized>
          <showzeroresults>F</showzeroresults>
          <headline>
            <reminder>
              <id>[scriptid=customsearch_ns_csgn_po_not_eligible]</id>
            </reminder>
          </headline>
        </reminders>
        <customsearch>
          <allowinlineediting>T</allowinlineediting>
          <backgroundtype>GLOBAL_BACKGROUND</backgroundtype>
          <charttheme>GLOBAL_THEME</charttheme>
          <drilldown>NEW_PAGE</drilldown>
          <isminimized>F</isminimized>
          <resultssize>10</resultssize>
          <savedsearch>[scriptid=customsearch_ns_csgn_ship_rejection]</savedsearch>
        </customsearch>
        <customsearch>
          <allowinlineediting>T</allowinlineediting>
          <backgroundtype>GLOBAL_BACKGROUND</backgroundtype>
          <charttheme>GLOBAL_THEME</charttheme>
          <drilldown>NEW_PAGE</drilldown>
          <isminimized>F</isminimized>
          <resultssize>10</resultssize>
          <savedsearch>[scriptid=customsearch_ns_csgn_weekly_received]</savedsearch>
        </customsearch>
      </leftcolumn>
      <centercolumn>
        <customsearch>
          <allowinlineediting>T</allowinlineediting>
          <backgroundtype>GLOBAL_BACKGROUND</backgroundtype>
          <charttheme>COLORFUL</charttheme>
          <drilldown>NEW_PAGE</drilldown>
          <isminimized>F</isminimized>
          <resultssize>10</resultssize>
          <savedsearch>[scriptid=customsearch_ns_csgn_sold_items]</savedsearch>
        </customsearch>
        <customsearch>
          <allowinlineediting>T</allowinlineediting>
          <backgroundtype>GLOBAL_BACKGROUND</backgroundtype>
          <charttheme>GLOBAL_THEME</charttheme>
          <drilldown>NEW_PAGE</drilldown>
          <isminimized>F</isminimized>
          <resultssize>10</resultssize>
          <savedsearch>[scriptid=customsearch_ns_csgn_to_tbd]</savedsearch>
        </customsearch>
        <customsearch>
          <allowinlineediting>T</allowinlineediting>
          <backgroundtype>GLOBAL_BACKGROUND</backgroundtype>
          <charttheme>GLOBAL_THEME</charttheme>
          <drilldown>NEW_PAGE</drilldown>
          <isminimized>F</isminimized>
          <resultssize>10</resultssize>
          <savedsearch>[scriptid=customsearch_ns_csgn_cosn_inventory]</savedsearch>
        </customsearch>
      </centercolumn>
    </dashboard>
  </dashboards>
</publisheddashboard>