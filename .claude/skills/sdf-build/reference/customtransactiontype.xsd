<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xsd:element name="customtransactiontype">
		<xsd:complexType>
			<xsd:all>
				<xsd:element name="name" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation>Max Length: 96. References: translationcollection.strings.string.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="96"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="transactionstyle" minOccurs="0" default="BASIC">
					<xsd:annotation>
						<xsd:documentation>Values: customtransactiontype_subliststyle.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[A-Z0-9_]+"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="subliststyle" minOccurs="0" default="BASIC">
					<xsd:annotation>
						<xsd:documentation>Values: customtransactiontype_subliststyle.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[A-Z0-9_]+"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="iscredit" minOccurs="0" default="F">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="T"/>
							<xsd:enumeration value="F"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="isposting" minOccurs="0" default="F">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="T"/>
							<xsd:enumeration value="F"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="showstatus" minOccurs="0" default="F">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="T"/>
							<xsd:enumeration value="F"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="isvoidable" minOccurs="0" default="F">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="T"/>
							<xsd:enumeration value="F"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="cancloseorder" minOccurs="0" default="F">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="T"/>
							<xsd:enumeration value="F"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="aidescription" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Max Length: 280.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="280"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="statuses" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="status" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:all>
										<xsd:element name="description" minOccurs="1">
											<xsd:annotation>
												<xsd:documentation>Max Length: 480. References: translationcollection.strings.string.</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="480"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="isposting" minOccurs="0" default="F">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:enumeration value="T"/>
													<xsd:enumeration value="F"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="id" minOccurs="1">
											<xsd:annotation>
												<xsd:documentation>Values: customtransactiontype_statuses_id.</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="[A-Z0-9_]+"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
									</xsd:all>
									<xsd:attribute name="scriptid" use="required">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:pattern value="_[a-zA-Z0-9_]+"/>
												<xsd:maxLength value="40"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="permissions" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="permission" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:all>
										<xsd:element name="permittedrole" minOccurs="1">
											<xsd:annotation>
												<xsd:documentation>References: role. Values: customrecordtype_permittedrole.</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="[A-Z0-9_]+"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="permittedlevel" minOccurs="1" default="NONE">
											<xsd:annotation>
												<xsd:documentation>Values: generic_permission_level.</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="[A-Z0-9_]+"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="links" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="link" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:all>
										<xsd:element name="linkcategory" minOccurs="1">
											<xsd:annotation>
												<xsd:documentation>Values: generic_centercategory.</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="[A-Z0-9_]+"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="linktasktype" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Values: customtransactiontype_tasktype.</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="[A-Z0-9_]+"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="linklabel" minOccurs="0" type="xsd:string">
											<xsd:annotation>
												<xsd:documentation>References: translationcollection.strings.string.</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="segments" minOccurs="0">
					<xsd:complexType>
						<xsd:all>
							<xsd:element name="classposition" minOccurs="0" default="NONE">
								<xsd:annotation>
									<xsd:documentation>Values: customtransactiontype_classification_position.</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:pattern value="[A-Z0-9_]+"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="isclassmandatory" minOccurs="0" default="F">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="T"/>
										<xsd:enumeration value="F"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="departmentposition" minOccurs="0" default="NONE">
								<xsd:annotation>
									<xsd:documentation>Values: customtransactiontype_classification_position.</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:pattern value="[A-Z0-9_]+"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="isdepartmentmandatory" minOccurs="0" default="F">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="T"/>
										<xsd:enumeration value="F"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="locationposition" minOccurs="0" default="NONE">
								<xsd:annotation>
									<xsd:documentation>Values: customtransactiontype_classification_position.</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:pattern value="[A-Z0-9_]+"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="islocationmandatory" minOccurs="0" default="F">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="T"/>
										<xsd:enumeration value="F"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="accounting" minOccurs="0">
					<xsd:complexType>
						<xsd:all>
							<xsd:element name="specifyaccountontransaction" minOccurs="0" default="F">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="T"/>
										<xsd:enumeration value="F"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="filterbyaccounttypeall" minOccurs="0" default="F">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="T"/>
										<xsd:enumeration value="F"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="filterbyaccounttype" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Values: customtransactiontype_filterbyaccounttype.</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:pattern value="[A-Z0-9_]+"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
			</xsd:all>
			<xsd:attribute name="scriptid" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:pattern value="_[a-zA-Z0-9_]+"/>
						<xsd:maxLength value="40"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
