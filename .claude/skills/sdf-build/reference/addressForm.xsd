<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xsd:element name="addressForm">
		<xsd:complexType>
			<xsd:all>
				<xsd:element name="name" minOccurs="1" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>References: translationcollection.strings.string.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="addressTemplate" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation>Max Length: 3990.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="3990"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="countries" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Values: countries.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[A-Z0-9_]+"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="mainFields" minOccurs="0">
					<xsd:complexType>
						<xsd:all>
							<xsd:element name="fieldGroup" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="label" minOccurs="1" type="xsd:string">
											<xsd:annotation>
												<xsd:documentation>References: translationcollection.strings.string.</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="visible" minOccurs="0" default="T">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:enumeration value="T"/>
													<xsd:enumeration value="F"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="showTitle" minOccurs="0" default="T">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:enumeration value="T"/>
													<xsd:enumeration value="F"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="singleColumn" minOccurs="0" default="F">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:enumeration value="T"/>
													<xsd:enumeration value="F"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="fields" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="field" minOccurs="0" maxOccurs="unbounded">
														<xsd:complexType>
															<xsd:all>
																<xsd:element name="id" minOccurs="1">
																	<xsd:annotation>
																		<xsd:documentation>References: crmcustomfield, customrecordtype.customrecordcustomfields.customrecordcustomfield, entitycustomfield, itemcustomfield, itemnumbercustomfield, itemoptioncustomfield, othercustomfield, transactionbodycustomfield, transactioncolumncustomfield. Values: addressform_fieldid.</xsd:documentation>
																	</xsd:annotation>
																	<xsd:simpleType>
																		<xsd:restriction base="xsd:string">
																			<xsd:pattern value="[A-Z0-9_]+"/>
																		</xsd:restriction>
																	</xsd:simpleType>
																</xsd:element>
																<xsd:element name="label" minOccurs="0" type="xsd:string">
																	<xsd:annotation>
																		<xsd:documentation>References: translationcollection.strings.string.</xsd:documentation>
																	</xsd:annotation>
																</xsd:element>
																<xsd:element name="visible" minOccurs="0" default="T">
																	<xsd:simpleType>
																		<xsd:restriction base="xsd:string">
																			<xsd:enumeration value="T"/>
																			<xsd:enumeration value="F"/>
																		</xsd:restriction>
																	</xsd:simpleType>
																</xsd:element>
																<xsd:element name="mandatory" minOccurs="0" default="F">
																	<xsd:simpleType>
																		<xsd:restriction base="xsd:string">
																			<xsd:enumeration value="T"/>
																			<xsd:enumeration value="F"/>
																		</xsd:restriction>
																	</xsd:simpleType>
																</xsd:element>
																<xsd:element name="displayType" minOccurs="0" default="NORMAL">
																	<xsd:annotation>
																		<xsd:documentation>Values: form_displaytype.</xsd:documentation>
																	</xsd:annotation>
																	<xsd:simpleType>
																		<xsd:restriction base="xsd:string">
																			<xsd:pattern value="[A-Z0-9_]+"/>
																		</xsd:restriction>
																	</xsd:simpleType>
																</xsd:element>
																<xsd:element name="columnBreak" minOccurs="0" default="F">
																	<xsd:simpleType>
																		<xsd:restriction base="xsd:string">
																			<xsd:enumeration value="T"/>
																			<xsd:enumeration value="F"/>
																		</xsd:restriction>
																	</xsd:simpleType>
																</xsd:element>
																<xsd:element name="spaceBefore" minOccurs="0" type="xsd:string"/>
																<xsd:element name="sameRowAsPrevious" minOccurs="0" default="F">
																	<xsd:simpleType>
																		<xsd:restriction base="xsd:string">
																			<xsd:enumeration value="T"/>
																			<xsd:enumeration value="F"/>
																		</xsd:restriction>
																	</xsd:simpleType>
																</xsd:element>
															</xsd:all>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
												<xsd:attribute name="position" use="optional" default="MIDDLE" type="xsd:string"/>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
									<xsd:attribute name="scriptid" use="required" type="xsd:string"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="defaultFieldGroup" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="fields" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="field" minOccurs="0" maxOccurs="unbounded">
														<xsd:complexType>
															<xsd:all>
																<xsd:element name="id" minOccurs="1">
																	<xsd:annotation>
																		<xsd:documentation>References: crmcustomfield, customrecordtype.customrecordcustomfields.customrecordcustomfield, entitycustomfield, itemcustomfield, itemnumbercustomfield, itemoptioncustomfield, othercustomfield, transactionbodycustomfield, transactioncolumncustomfield. Values: addressform_fieldid.</xsd:documentation>
																	</xsd:annotation>
																	<xsd:simpleType>
																		<xsd:restriction base="xsd:string">
																			<xsd:pattern value="[A-Z0-9_]+"/>
																		</xsd:restriction>
																	</xsd:simpleType>
																</xsd:element>
																<xsd:element name="label" minOccurs="0" type="xsd:string">
																	<xsd:annotation>
																		<xsd:documentation>References: translationcollection.strings.string.</xsd:documentation>
																	</xsd:annotation>
																</xsd:element>
																<xsd:element name="visible" minOccurs="0" default="T">
																	<xsd:simpleType>
																		<xsd:restriction base="xsd:string">
																			<xsd:enumeration value="T"/>
																			<xsd:enumeration value="F"/>
																		</xsd:restriction>
																	</xsd:simpleType>
																</xsd:element>
																<xsd:element name="mandatory" minOccurs="0" default="F">
																	<xsd:simpleType>
																		<xsd:restriction base="xsd:string">
																			<xsd:enumeration value="T"/>
																			<xsd:enumeration value="F"/>
																		</xsd:restriction>
																	</xsd:simpleType>
																</xsd:element>
																<xsd:element name="displayType" minOccurs="0" default="NORMAL">
																	<xsd:annotation>
																		<xsd:documentation>Values: form_displaytype.</xsd:documentation>
																	</xsd:annotation>
																	<xsd:simpleType>
																		<xsd:restriction base="xsd:string">
																			<xsd:pattern value="[A-Z0-9_]+"/>
																		</xsd:restriction>
																	</xsd:simpleType>
																</xsd:element>
																<xsd:element name="columnBreak" minOccurs="0" default="F">
																	<xsd:simpleType>
																		<xsd:restriction base="xsd:string">
																			<xsd:enumeration value="T"/>
																			<xsd:enumeration value="F"/>
																		</xsd:restriction>
																	</xsd:simpleType>
																</xsd:element>
																<xsd:element name="spaceBefore" minOccurs="0" type="xsd:string"/>
																<xsd:element name="sameRowAsPrevious" minOccurs="0" default="F">
																	<xsd:simpleType>
																		<xsd:restriction base="xsd:string">
																			<xsd:enumeration value="T"/>
																			<xsd:enumeration value="F"/>
																		</xsd:restriction>
																	</xsd:simpleType>
																</xsd:element>
															</xsd:all>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
												<xsd:attribute name="position" use="optional" default="MIDDLE" type="xsd:string"/>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="customCode" minOccurs="0">
					<xsd:complexType>
						<xsd:all>
							<xsd:element name="scriptFile" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:pattern value="\[/[A-Za-z0-9._\-/]+\]"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
			</xsd:all>
			<xsd:attribute name="scriptid" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:pattern value="custform_[a-zA-Z0-9_]+"/>
						<xsd:maxLength value="99"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="standard" use="optional">
				<xsd:annotation>
					<xsd:documentation>Max Length: 99</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="99"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
