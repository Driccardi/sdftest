<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xsd:element name="subtab">
		<xsd:complexType>
			<xsd:all>
				<xsd:element name="title" minOccurs="1" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>References: translationcollection.strings.string.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="tabtype" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation>Values: generic_tab_type.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[A-Z0-9_]+"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="parent" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>References: subtab. Values: generic_tab_parent.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[A-Z0-9_]+"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
			</xsd:all>
			<xsd:attribute name="scriptid" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:pattern value="custtab_[a-zA-Z0-9_]+"/>
						<xsd:maxLength value="40"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
