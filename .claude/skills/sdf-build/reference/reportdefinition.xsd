<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xsd:element name="reportdefinition">
		<xsd:complexType>
			<xsd:all>
				<xsd:element name="name" minOccurs="1" type="xsd:string"/>
				<xsd:element name="definition" minOccurs="1" type="xsd:string"/>
				<xsd:element name="audience" minOccurs="0">
					<xsd:complexType>
						<xsd:all>
							<xsd:element name="allcustomers" minOccurs="0" default="F">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="T"/>
										<xsd:enumeration value="F"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="allemployees" minOccurs="0" default="F">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="T"/>
										<xsd:enumeration value="F"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="allpartners" minOccurs="0" default="F">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="T"/>
										<xsd:enumeration value="F"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="allroles" minOccurs="0" default="F">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="T"/>
										<xsd:enumeration value="F"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="allvendors" minOccurs="0" default="F">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="T"/>
										<xsd:enumeration value="F"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="audslctrole" minOccurs="0" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>References: role.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="accessaudience" minOccurs="0">
					<xsd:complexType>
						<xsd:all>
							<xsd:element name="allcustomers" minOccurs="0" default="F">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="T"/>
										<xsd:enumeration value="F"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="allemployees" minOccurs="0" default="F">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="T"/>
										<xsd:enumeration value="F"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="allpartners" minOccurs="0" default="F">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="T"/>
										<xsd:enumeration value="F"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="allroles" minOccurs="0" default="F">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="T"/>
										<xsd:enumeration value="F"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="allvendors" minOccurs="0" default="F">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="T"/>
										<xsd:enumeration value="F"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="audslctrole" minOccurs="0" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>References: role.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
			</xsd:all>
			<xsd:attribute name="scriptid" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:pattern value="customreport_[a-zA-Z0-9_]+"/>
						<xsd:maxLength value="40"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
