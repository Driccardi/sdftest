<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xsd:element name="cmscontenttype">
		<xsd:complexType>
			<xsd:all>
				<xsd:element name="name" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation>Max Length: 999.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="999"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="label" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation>Max Length: 18.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="18"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="customrecordid" minOccurs="1" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>References: customrecordtype.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="description" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation>Max Length: 999.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="999"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="iconimagepath" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Max Length: 999.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="999"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
			</xsd:all>
			<xsd:attribute name="scriptid" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:pattern value="custcontenttype_[a-zA-Z0-9_]+"/>
						<xsd:maxLength value="99"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
