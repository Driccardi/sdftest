<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xsd:element name="customlist">
		<xsd:complexType>
			<xsd:all>
				<xsd:element name="name" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation>Max Length: 30. References: translationcollection.strings.string.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="30"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="owner" minOccurs="0" type="xsd:string"/>
				<xsd:element name="description" minOccurs="1" type="xsd:string"/>
				<xsd:element name="isinactive" minOccurs="0" default="F">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="T"/>
							<xsd:enumeration value="F"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="ismatrixoption" minOccurs="0" default="F">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="T"/>
							<xsd:enumeration value="F"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="isordered" minOccurs="0" default="T">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="T"/>
							<xsd:enumeration value="F"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="customvalues" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="customvalue" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:all>
										<xsd:element name="abbreviation" minOccurs="0" type="xsd:string"/>
										<xsd:element name="isinactive" minOccurs="0" default="F">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:enumeration value="T"/>
													<xsd:enumeration value="F"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="value" minOccurs="1" type="xsd:string">
											<xsd:annotation>
												<xsd:documentation>References: translationcollection.strings.string.</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:all>
									<xsd:attribute name="scriptid" use="required">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:pattern value="_[a-zA-Z0-9_]+"/>
												<xsd:maxLength value="40"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:all>
			<xsd:attribute name="scriptid" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:pattern value="customlist_[a-zA-Z0-9_]+"/>
						<xsd:maxLength value="38"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
