<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xsd:element name="centertab">
		<xsd:complexType>
			<xsd:all>
				<xsd:element name="label" minOccurs="1" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>References: translationcollection.strings.string.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="allvendors" minOccurs="0" default="F">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="T"/>
							<xsd:enumeration value="F"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="allroles" minOccurs="0" default="F">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="T"/>
							<xsd:enumeration value="F"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="allpartners" minOccurs="0" default="F">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="T"/>
							<xsd:enumeration value="F"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="allcustomers" minOccurs="0" default="F">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="T"/>
							<xsd:enumeration value="F"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="allemployees" minOccurs="0" default="F">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="T"/>
							<xsd:enumeration value="F"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="audslctrole" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>References: role. Values: generic_role.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[A-Z0-9_]+"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="center" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation>References: center. Values: generic_centertype.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[A-Z0-9_]+"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="portlets" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="portlet" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:all>
										<xsd:element name="portlet" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Values: generic_portlet.</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="[A-Z0-9_]+"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="portletcolumn" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Values: generic_portletcolumn.</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="[A-Z0-9_]+"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="isportletshown" minOccurs="0" default="T">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:enumeration value="T"/>
													<xsd:enumeration value="F"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
									</xsd:all>
									<xsd:attribute name="scriptid" use="required">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:pattern value="_[a-zA-Z0-9_]+"/>
												<xsd:maxLength value="40"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:all>
			<xsd:attribute name="scriptid" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:pattern value="custcentertab_[a-zA-Z0-9_]+"/>
						<xsd:maxLength value="99"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
