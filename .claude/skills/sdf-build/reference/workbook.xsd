<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xsd:element name="workbook">
		<xsd:complexType>
			<xsd:all>
				<xsd:element name="name" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation>Max Length: 50. References: translationcollection.strings.string.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="50"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="definition" minOccurs="1" type="xsd:string"/>
				<xsd:element name="tables" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="table" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:attribute name="scriptid" use="required">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:pattern value="custview_[a-zA-Z0-9_]+"/>
												<xsd:maxLength value="99"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="charts" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="chart" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:attribute name="scriptid" use="required">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:pattern value="custchart_[a-zA-Z0-9_]+"/>
												<xsd:maxLength value="99"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="pivots" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="pivot" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:attribute name="scriptid" use="required">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:pattern value="custpivot_[a-zA-Z0-9_]+"/>
												<xsd:maxLength value="99"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:all>
			<xsd:attribute name="scriptid" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:pattern value="custworkbook_[a-zA-Z0-9_]+"/>
						<xsd:maxLength value="99"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
