					<xsd:annotation>
						<xsd:documentation>Values: generic_tab_type.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[A-Z0-9_]+"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="tab" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>References: subtab. Values: generic_tab_parent.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[A-Z0-9_]+"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="field" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>References: crmcustomfield, entitycustomfield, itemcustomfield, transactionbodycustomfield. Values: sublist_standard_fields.</xsd:documentation>
