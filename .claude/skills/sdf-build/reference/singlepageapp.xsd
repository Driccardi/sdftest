<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xsd:element name="singlepageapp">
		<xsd:complexType>
			<xsd:all>
				<xsd:element name="name" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation>Max Length: 1000.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="1000"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="description" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation>Max Length: 1000.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="1000"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="url" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation>Max Length: 1000.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="1000"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="clientscriptfile" minOccurs="1" type="xsd:string"/>
				<xsd:element name="serverscriptfile" minOccurs="1" type="xsd:string"/>
				<xsd:element name="assetsfolder" minOccurs="0" type="xsd:string"/>
				<xsd:element name="folder" minOccurs="1" type="xsd:string"/>
				<xsd:element name="audienceallroles" minOccurs="0" default="F">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="T"/>
							<xsd:enumeration value="F"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="loglevel" minOccurs="0" default="DEBUG">
					<xsd:annotation>
						<xsd:documentation>Values: script_loglevel.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[A-Z0-9_]+"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="audienceroles" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>References: role. Values: generic_role.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[A-Z0-9_]+"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="executeas" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>References: role. Values: generic_role.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[A-Z0-9_]+"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="usesuif" minOccurs="0" default="T">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="T"/>
							<xsd:enumeration value="F"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="notifyadmins" minOccurs="0" default="F">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="T"/>
							<xsd:enumeration value="F"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="notifyowner" minOccurs="0" default="T">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="T"/>
							<xsd:enumeration value="F"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="notifyuser" minOccurs="0" default="F">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="T"/>
							<xsd:enumeration value="F"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="notifyemails" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Max Length: 999.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="999"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="links" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="link" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:all>
										<xsd:element name="linkcategory" minOccurs="1">
											<xsd:annotation>
												<xsd:documentation>Values: generic_centercategory.</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:pattern value="[A-Z0-9_]+"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="linklabel" minOccurs="1">
											<xsd:annotation>
												<xsd:documentation>Max Length: 100. References: translationcollection.strings.string.</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="100"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:all>
			<xsd:attribute name="scriptid" use="required">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:pattern value="custspa_[a-zA-Z0-9_]+"/>
						<xsd:maxLength value="28"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
